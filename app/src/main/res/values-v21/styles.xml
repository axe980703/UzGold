<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="MyTheme" parent="@android:style/Theme.Material.Light">
        <item name="android:actionBarStyle">@style/MyActionBar</item>
    </style>

    <style name="MyActionBar" parent="@android:style/Widget.Material.Light.ActionBar">
        <item name="android:background">#3ca3ff</item>
        <item name="android:icon">@android:color/transparent</item>
    </style>

    <style name="JTheme" parent="Theme.AppCompat.Light">
        <item name="android:actionBarStyle">@style/MyActionBar</item>
        <item name="colorPrimary">#87CAB3</item>
        <item name="android:statusBarColor">#50AF91</item>
        <item name="colorAccent">#00796B</item>

    </style>
</resources>
